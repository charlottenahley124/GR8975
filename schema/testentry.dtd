<!ELEMENT entry (#PCDATA|block|figure|folio|heading|purpose|list|annotations)*>
<!ATTLIST entry
  xmlns CDATA #FIXED ''
  group NMTOKEN #IMPLIED
  identifier NMTOKEN #IMPLIED>

<!ELEMENT entry_group (heading?,entry+)>
<!ATTLIST entry_group
  identifier NMTOKEN #REQUIRED>

<!ELEMENT list (foreign|item+)>
<!ATTLIST list
  xmlns CDATA #FIXED ''
  purpose NMTOKEN #IMPLIED>

<!ELEMENT plant (#PCDATA)>
<!ATTLIST plant
  xmlns CDATA #FIXED ''>

<!ELEMENT figure EMPTY>
<!ATTLIST figure
  xmlns CDATA #FIXED ''
  identifier NMTOKEN #IMPLIED
  number NMTOKEN #IMPLIED
  position NMTOKEN #IMPLIED
  url CDATA #IMPLIED>

<!ELEMENT heading (#PCDATA|activity|material)*>
<!ATTLIST heading
  xmlns CDATA #FIXED ''
  position NMTOKEN #IMPLIED>

<!ELEMENT purpose (#PCDATA|activity|material|material_format|profession
                   |ref|sub_recipe|tool|unit)*>
<!ATTLIST purpose
  xmlns CDATA #FIXED ''
  type NMTOKEN #REQUIRED>

<!ELEMENT profession (#PCDATA)>
<!ATTLIST profession
  xmlns CDATA #FIXED ''>

<!ELEMENT place (#PCDATA)>
<!ATTLIST place
  xmlns CDATA #FIXED ''>

<!ELEMENT folio EMPTY>
<!ATTLIST folio
  xmlns CDATA #FIXED ''
  end NMTOKEN #IMPLIED
  image CDATA #IMPLIED
  number NMTOKEN #IMPLIED
  start NMTOKEN #IMPLIED
  url CDATA #IMPLIED>

<!ELEMENT block (#PCDATA|activity|animal|figure|foreign
                 |material|material_format|place|plant
                 |profession|image|purpose|ref|sub_recipe|tool|unit|color|name|q
                 |sup|time)*>
<!ATTLIST block
  xmlns CDATA #FIXED ''
  identifier NMTOKEN #IMPLIED
  pisiton NMTOKEN #IMPLIED
  position NMTOKEN #IMPLIED
  purpose CDATA #IMPLIED>

<!ELEMENT color (#PCDATA)>
<!ATTLIST color
  xmlns CDATA #FIXED ''>

<!ELEMENT name (#PCDATA)>
<!ATTLIST name
  xmlns CDATA #FIXED ''
  type NMTOKEN #REQUIRED>

<!ELEMENT q (foreign)>
<!ATTLIST q
  xmlns CDATA #FIXED ''>

<!ELEMENT sup (#PCDATA|material)*>
<!ATTLIST sup
  xmlns CDATA #FIXED ''>

<!ELEMENT time (#PCDATA)>
<!ATTLIST time
  xmlns CDATA #FIXED ''>


<!ELEMENT material (#PCDATA|animal | foreign | place)*>
<!ATTLIST material
  xmlns CDATA #FIXED ''
  type NMTOKEN #IMPLIED>

<!ELEMENT animal (#PCDATA)>
<!ATTLIST animal
  xmlns CDATA #FIXED ''>

<!ELEMENT tool (#PCDATA|material|tool)*>
<!ATTLIST tool
  xmlns CDATA #FIXED ''>

<!ELEMENT foreign (#PCDATA|item)*>
<!ATTLIST foreign
  xmlns CDATA #FIXED ''
  language NMTOKEN #REQUIRED>

<!ELEMENT item (#PCDATA|material|tool|unit)*>
<!ATTLIST item
  xmlns CDATA #FIXED ''>

<!ELEMENT activity (#PCDATA|activity|material)*>
<!ATTLIST activity
  type NMTOKEN #REQUIRED >

<!ELEMENT material_format (#PCDATA|material)*>
<!ATTLIST material_format
  xmlns CDATA #FIXED ''>

<!ELEMENT ref (#PCDATA)>
<!ATTLIST ref
  xmlns CDATA #FIXED ''
  identifier NMTOKEN #IMPLIED
  type NMTOKEN #IMPLIED>

<!ELEMENT sub_recipe (#PCDATA|foreign|material|tool)*>
<!ATTLIST sub_recipe
  xmlns CDATA #FIXED ''
  type NMTOKEN #IMPLIED>

<!ELEMENT image EMPTY>
<!ATTLIST image
  xmlns CDATA #FIXED ''
  identifier NMTOKEN #REQUIRED
  position NMTOKEN #IMPLIED
  url CDATA #REQUIRED>

<!ELEMENT unit (#PCDATA|tool)*>
<!ATTLIST unit
  xmlns CDATA #FIXED ''>

<!ELEMENT annotations (annotation)+>
<!ATTLIST annotations >

<!ELEMENT annotation EMPTY>
<!ATTLIST annotation
  title CDATA #REQUIRED
  url CDATA #REQUIRED >
